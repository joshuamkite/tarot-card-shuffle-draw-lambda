# AWS Configuration
aws_region  = "us-east-1"
environment = "dev"

# Project Configuration
project_name = "tarot"

# CloudFront Origin Access Control
origin_access_control_name = "TarotImages"

# API Gateway Throttling Settings
default_throttling_rate_limit  = 100
default_throttling_burst_limit = 200

# Lambda Configuration
lambda_timeout     = 10
lambda_memory_size = 128

# CloudWatch Logs
log_retention_days = 7